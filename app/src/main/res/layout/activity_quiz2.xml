<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_quiz2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="id.sch.smktelkom_mlg.project.xirpl102112029.wakemeup.QuizActivity2">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Jawab dan Matikan alarm" />


    <TextView
        android:id="@+id/soal2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Yang bukan kawasan di eropa" />

    <Button
        android:id="@+id/jaw2a"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Scandinavia" />

    <Button
        android:id="@+id/jaw2b"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Benelux" />

    <Button
        android:id="@+id/jaw2c"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Great Britain" />

    <Button
        android:id="@+id/jaw2d"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="East Siberia" />
</LinearLayout>
